{
    "name": "muso-ai",
    "version": "1.0.0",
    "private": true,
    "engines": {
        "node": ">=8.11.3"
    },
    "scripts": {
        "dev": "cross-env NODE_ENV=development nodemon ./server.js",
        "prod": "cross-env NODE_ENV=production node ./server.js",
        "build": "rm -rf ./dist && npm run build:client && npm run build:server",
        "bundle-report": "cross-env NODE_ENV=development webpack --config ./webpack/webpack.client.config.js --profile --json > stats.json && webpack-bundle-analyzer stats.json",
        "build:client": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.client.config.js",
        "build:server": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.server.config.js"
    },
